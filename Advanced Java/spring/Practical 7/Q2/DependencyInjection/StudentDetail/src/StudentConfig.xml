<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<!-- Injecting dependency using value tag -->
	<bean id="s1" class="com.Student.StudentInfo" >
		<property name="name">
			<value> Baburao Apte </value>
		</property>
		<property name="rollNo">
			<value> 101 </value>
		</property>
		<property name="std">
			<value> FYMCA </value>
		</property>
	</bean>
	
	<!-- Injecting dependency using value attribute -->
	
	<bean id="s2" class="com.Student.StudentInfo" >
		<property name="name" value="Babu Hatela" />
		<property name="rollNo" value="102" />
		<property name="std" value="SYMCA" />
	</bean>
	
	<!-- Injecting dependency using p attribute -->
	<bean id="s3" class="com.Student.StudentInfo" p:name="Ganpat Ghisela" p:rollNo="103" p:std="TYMCA" />

</beans>
